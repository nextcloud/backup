    "@nextcloud/l10n": "^2.2.0",
    "@nextcloud/logger": "^2.7.0",
    "@nextcloud/moment": "^1.3.1",
    "@nextcloud/router": "^3.0.1",
    "@nextcloud/vue": "^8.12.0",
    "vue": "^2.7.16",
    "vue-material-design-icons": "^5.3.0"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
